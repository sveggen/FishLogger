
<div class="ui centered cards">
  {{#each developers}}
  <div class="ui card" id ="{{name}}">
  <div class="image">
    <img src="{{image}}">
  </div>
  <div class="content">
    <a class="header">{{fullname}}</a>
    <div class="meta">
      <span class="date">Joined in {{joined}}</span>
    </div>
    <div class="description">
      {{name}} is a {{job}} in {{city}}
    </div>
  </div>
  <div class="extra content">
  </div>
</div>
  {{/each}}
</div>